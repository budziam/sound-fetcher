{
  "name": "seek/sound-fetcher",
  "description": "Sound fetcher",
  "keywords": [],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "vlucas/phpdotenv": "~1.0",
    "google/apiclient": "1.0.*@beta"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Src\\": "src/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
